# ~/.ssh/config

Host do01sgp1
    Hostname {{ pass "server/do01sgp1" }}
    Port {{ pass "protocol/ssh/port" }}

Host homelab
    Hostname {{ pass "server/homelab" }}
    # AddKeysToAgent yes
    # ForwardAgent yes
    # RemoteForward /run/user/1000/gnupg/S.gpg-agent /Users/nnd/.gnupg/S.gpg-agent.extra

Host dlab
    Hostname 192.168.12.14

Host dlab homelab
    User nnd

Host *
    User root
    Compression yes
    TCPKeepAlive yes
